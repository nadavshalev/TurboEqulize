function [dn] = EncryptorPath2(codedBits)
    % using LDPC and helscanintrlv
    
    symbs = symbMap(codedBits) / sqrt(2);
    
    % Interleaving
    nIntrlvRows = 10;
    nIntrlvCols = nLDPC/nIntrlvRows;
    intrlvHstep = 3; % hstep is the slope of the diagonal, that is, the amount by which
    %the row index increases as the column index increases by one

    intrlvRes = helscanintrlv(symbs,nIntrlvRows,nIntrlvCols,intrlvHstep);
end

